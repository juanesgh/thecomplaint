<div class = "c">
    <table class = "scroll">
        <tr>
            <th>User Name</th>
        </tr>
        <% @usr.each do |u| %>
            <tr>
                <% if !u.superadmin or u.superadmin == nil %>
                    <td><%= link_to u.name, geofences_viewadmin_path(:user_id => u.id), class: "btn btn-primary btn-md" %></td>
                <% end %>
            </tr>
        <% end %>
    </table>
</div>